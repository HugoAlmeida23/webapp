{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "buildCommand": "rm -rf node_modules && npm install && npm run build"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "github": {
    "enabled": true,
    "silent": true,
    "autoJobCancelation": true
  },
  "bypassCache": true
}
